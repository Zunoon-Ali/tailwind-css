#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Zunoon/OneDrive/Desktop/test/node_modules/browserslist/node_modules:/mnt/c/Users/Zunoon/OneDrive/Desktop/test/node_modules:/mnt/c/Users/Zunoon/OneDrive/Desktop/node_modules:/mnt/c/Users/Zunoon/OneDrive/node_modules:/mnt/c/Users/Zunoon/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Zunoon/OneDrive/Desktop/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Zunoon/OneDrive/Desktop/test/node_modules/browserslist/node_modules:/mnt/c/Users/Zunoon/OneDrive/Desktop/test/node_modules:/mnt/c/Users/Zunoon/OneDrive/Desktop/node_modules:/mnt/c/Users/Zunoon/OneDrive/node_modules:/mnt/c/Users/Zunoon/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Zunoon/OneDrive/Desktop/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browserslist/cli.js" "$@"
else
  exec node  "$basedir/../browserslist/cli.js" "$@"
fi
